{
  "semi": true,
  "singleQuote": true,
  "printWidth": 100,
  "tabWidth": 2,
  "trailingComma": "es5",
  "arrowParens": "always",
  "endOfLine": "crlf",
  "plugins": [
    "@trivago/prettier-plugin-sort-imports"
  ],
  "importOrder": [
    "^node:",
    "^@nestjs/",
    "^[^@\\.]",
    "^@krgeobuk/",
    "^@common/",
    "^@config$",
    "^@config/",
    "^@database$",
    "^@database/",
    "^@modules/",
    "^@/",
    "^\\.\\.\\./",
    "^\\./[^/]+$",
    "^\\."
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
